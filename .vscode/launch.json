{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Elastc",
      "program": "natours/server.js",
      "useWSL": true,
      "autoAttachChildProcesses": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "outputCapture": "std",
      "preLaunchTask": "clean-debug",
      "postDebugTask": "clean-debug"
    }
  ]
}
